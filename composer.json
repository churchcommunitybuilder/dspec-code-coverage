{
  "name": "ccb/dspec-code-coverage",
  "description": "Add code coverage to dspec",
  "license": "MIT",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/churchcommunitybuilder/dspec"
    }
  ],
  "require": {
    "davedevelopment/dspec": "dev-master",
    "phpunit/php-code-coverage": "~5.0"
  },
  "require-dev": {
    "symfony/yaml": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "CCB\\DspecCodeCoverage\\" : "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.9.x-dev"
    }
  }
}
